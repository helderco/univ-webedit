{% extends 'FOSUserBundle::layout.html.twig' %}

{% block title %}{{ 'layout.register'|trans }}{% endblock %}

{% form_theme form theme %}

{% block fos_user_content %}
<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" id="fos_user_registration_register">
    {{ form_widget(form) }}
    <input type="submit" value="{{ 'registration.submit'|trans }}" />
</form>
{% endblock fos_user_content %}
